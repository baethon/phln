{
    "name": "baethon/phln",
    "description": "Set of small utility functions. Inspired by Ramda.",
    "type": "library",
    "require": {
        "php": ">=7.4",
        "ayesh/is_countable-polyfill": "^1.0"
    },
    "require-dev": {
        "symfony/console": "^3.2",
        "illuminate/view": "^5.4",
        "illuminate/console": "^5.4",
        "symfony/process": "^3.3",
        "phpdocumentor/reflection-docblock": "^3.1",
        "phpunit/phpunit": "^6.5",
        "doctrine/instantiator": "~1.0.5",
        "squizlabs/php_codesniffer": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "phln\\": "src",
            "Baethon\\Phln\\": "src"
        },
        "files": [
            "src/functions.php",
            "src/bundle.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Phln\\Build\\": "build"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Radoslaw Mejer",
            "email": "radmen@radmen.info"
        }
    ],
    "scripts": {
        "lint": [
          "./vendor/bin/phpcs",
          "phpstan analyse"
        ],
        "test": [
            "./vendor/bin/phpunit"
        ]
    }
}
