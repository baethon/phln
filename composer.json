{
    "name": "baethon/phln",
    "description": "A practical functional library for PHP developers",
    "type": "library",
    "require": {
        "ayesh/is_countable-polyfill": "^1.0",
        "spatie/macroable": "^1.0"
    },
    "scripts": {
        "build": [
            "./bin/console.php create:bundle",
            "./bin/console.php create:docs"
        ]
    },
    "require-dev": {
        "symfony/console": "^3.2",
        "illuminate/view": "^5.4",
        "illuminate/console": "^5.4",
        "symfony/process": "^3.3",
        "phpdocumentor/reflection-docblock": "^3.1",
        "phpunit/phpunit": "^6.5",
        "doctrine/instantiator": "~1.0.5"
    },
    "autoload": {
        "psr-4": {
            "phln\\": "src"
        },
        "files": [
            "src/bundle.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Phln\\Build\\": "build"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Radoslaw Mejer",
            "email": "radmen@radmen.info"
        }
    ]
}
