{
    "name": "baethon/phln",
    "description": "Set of small utility functions. Inspired by Ramda.",
    "type": "library",
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.2"
    },
    "autoload": {
        "psr-4": {
            "Baethon\\Phln\\": "src"
        },
        "files": [
            "src/bundle.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Phln\\Build\\": "build"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Radoslaw Mejer",
            "email": "radmen@radmen.info"
        }
    ],
    "scripts": {
        "lint": [
          "./vendor/bin/phpcs",
          "phpstan analyse"
        ],
        "test": [
            "./vendor/bin/phpunit"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
